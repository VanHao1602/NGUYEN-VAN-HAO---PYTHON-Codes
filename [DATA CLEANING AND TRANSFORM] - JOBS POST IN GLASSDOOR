        "# Split bằng ký tự '-'\n",
        "dataRaw['min_salary'] = dataRaw['Salary Estimate'].apply(lambda x: int(x.split('-')[0]))\n",
        "# Lấy ký tự đầu tiên (index 0) của List sau khi tách là giá trị min đồng thời chuyển về dữ liệu dạng int\n",
        "dataRaw['max_salary'] = dataRaw['Salary Estimate'].apply(lambda x: int(x.split('-')[1]))\n",
        "# Lấy ký tự thứ hai (index 1) của List sau khi tách là giá trị max đồng thời chuyển về dữ liệu dạng int\n",
        "dataRaw.head()"
