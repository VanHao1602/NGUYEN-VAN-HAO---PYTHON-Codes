      },
      "outputs": [
        {
          "output_type": "execute_result",
          "data": {
            "text/plain": [
              "NA        576\n",
              "Senior     94\n",
              "Junior      2\n",
              "Name: Job_Level, dtype: int64"
            ]
          },
          "metadata": {},
          "execution_count": 42
        }
      ],
      "source": [
        "dataRaw['Job_Level'] = dataRaw['Job Title'].apply(Level_classifier)\n",
        "dataRaw['Job_Level'].value_counts()"
      ]
