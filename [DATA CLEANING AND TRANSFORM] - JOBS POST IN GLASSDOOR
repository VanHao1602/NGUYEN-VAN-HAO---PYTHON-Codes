      "source": [
        "# Xây dựng hàm trích xuất level của từng Job title\n",
        "def Level_classifier(title):\n",
        "    if 'sr' in title.lower() or 'senior' in title.lower() or 'sr' in title.lower() or 'lead' in title.lower() or 'principal' in title.lower():\n",
        "            return 'Senior'\n",
        "    elif 'jr' in title.lower() or 'jr.' in title.lower():\n",
        "        return 'Junior'\n",
        "    else:\n",
        "        return 'NA'"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": 42,
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "id": "QFkX8OSA5bYQ",
